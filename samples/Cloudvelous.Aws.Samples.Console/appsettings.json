{
  "AWS": {
    "Region": "us-east-1"
  },
  "Sqs": {
    "DefaultVisibilityTimeoutSeconds": 30,
    "MaxReceiveMessages": 10,
    "DefaultReceiveMessageWaitTimeSeconds": 20
  },
  "SecretsManager": {
    "DefaultCacheDurationMinutes": 60,
    "EnableCaching": true,
    "MaxCacheSize": 1000
  },
  "Rds": {
    "DefaultPort": 1433,
    "DefaultConnectionTimeoutSeconds": 30,
    "DefaultCommandTimeoutSeconds": 30,
    "UseSsl": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
